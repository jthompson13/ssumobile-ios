
platform :ios, "8.4"

source 'https://github.com/CocoaPods/Specs.git'

inhibit_all_warnings!

use_frameworks!

target "SSUMobile" do
    pod 'Kingfisher', '~> 3.6'
    pod 'RBStoryboardLink', '~> 0.1.0'
    pod 'MBProgressHUD','~> 1.0'
    pod 'FSCalendar', '~> 2.7.4'
    pod 'Masonry'
    pod 'MJDijkstra', '~> 1.0.0'
    pod 'Mockingjay/Core', '~> 2.0'
    pod 'CocoaLumberjack/Swift', '~> 3.2'
    pod 'SimpleKeychain', '~> 0.6'
    pod 'SnapKit', '~> 3.2'
    pod 'SwiftyJSON', '~> 3.1'
    
    target "SSUMobileTests" do
        inherit! :search_paths
        pod 'Quick', '~> 1.1'
        pod 'Nimble', '~> 6.1'
        pod 'Mockingjay/XCTest', '~> 2.0'
    end
end

